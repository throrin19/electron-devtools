{
  "private": true,
  "name": "electron-toolbox",
  "description": "Many developpers tools in one electron app. Like a toolbox",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "postinstall": "cd app && npm install && rm -rf package-lock.json",
    "test": "echo lol",
    "build": "npx poi build",
    "dev": "npx poi",
    "both": "concurrently \"npm run dev\" \"npm run app\" -k",
    "app": "electron app/index.js",
    "dist": "npm run dist:mac && npm run dist:deb && npm run dist:win",
    "dist:travis": "npm run dist:deb && npm run dist:win",
    "dist:mac": "build --mac",
    "dist:deb": "build --linux deb tar.xz",
    "dist:win": "build --win --x64"
  },
  "build": {
    "appId": "com.throrin19.electron-toolbox",
    "productName": "Toolbox",
    "asar": true,
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "category": "public.app-category.developer-tools"
    }
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "concurrently": "3.5.1",
    "electron": "1.7.11",
    "electron-builder": "19.54.0",
    "electron-builder-squirrel-windows": "19.55.1",
    "eslint": "4.16.0",
    "eslint-config-throrinstudio": "1.2.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-vue": "4.2.0",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "node-sass": "4.7.2",
    "poi": "9.6.13",
    "poi-preset-eslint": "9.1.1",
    "sass-loader": "6.0.6",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "vue-eslint-parser": "2.0.2"
  },
  "dependencies": {
    "jsoneditor": "5.13.2",
    "lodash": "4.17.4",
    "moment": "2.20.1",
    "ms": "2.1.1",
    "muse-ui": "2.1.0",
    "roboto-fontface": "0.8.0",
    "v-clipboard": "1.0.4",
    "vue-router": "3.0.1",
    "vuetify": "0.17.6"
  }
}
